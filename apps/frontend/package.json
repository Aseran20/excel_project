{
  "name": "@algosheet/frontend",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Excel-Custom-Functions.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16 <21",
    "npm": ">=7 <11"
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "convert-to-single-host": "node convertToSingleHost.js",
    "dev-server": "webpack serve --mode development",
    "lint": "cd ../.. && biome check apps/frontend/src/",
    "lint:fix": "cd ../.. && biome check --write apps/frontend/src/",
    "format": "cd ../.. && biome format --write apps/frontend/src/",
    "prestart": "npm run build",
    "signin": "office-addin-dev-settings m365-account login",
    "signout": "office-addin-dev-settings m365-account logout",
    "start": "office-addin-debugging start manifest.xml",
    "stop": "office-addin-debugging stop manifest.xml",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "mocha -r ts-node/register test/end-to-end/*.ts",
    "test:unit": "mocha -r ts-node/register test/unit/*.test.ts",
    "validate": "office-addin-manifest validate manifest.xml",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "@algosheet/shared": "*",
    "@trpc/client": "^11.7.2",
    "core-js": "^3.36.0",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.23.3",
    "@types/custom-functions-runtime": "^1.6.8",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.25",
    "@types/office-js": "^1.0.377",
    "@types/office-runtime": "^1.0.35",
    "acorn": "^8.5.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "custom-functions-metadata-plugin": "^2.1.2",
    "file-loader": "^6.2.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mocha": "^11.1.0",
    "office-addin-cli": "^2.0.3",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-manifest": "^2.0.3",
    "office-addin-mock": "^3.0.3",
    "office-addin-test-helpers": "^2.0.3",
    "office-addin-test-server": "^2.0.3",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "request": "^2.88.2",
    "source-map-loader": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "5.1.0"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ]
}
